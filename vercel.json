{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=0"
        },
        {
          "key": "Accept-Ranges",
          "value": "none"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.mybookseries.org/$1",
      "has": [
        {
          "type": "host",
          "value": "mybookseries.org"
        }
      ],
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/book", "destination": "/book.html" },
    { "source": "/chapter", "destination": "/chapter.html" },
    { "source": "/wallet", "destination": "/wallet.html" }
  ]
}
